{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "watch": false,
    "declaration": true,
    "sourceMap": true,
    "outDir": "./bin",
    "rootDir": "./src",
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "noImplicitThis": false,
    "alwaysStrict": false,
    "stripInternal": false,
    "importHelpers": false,
    "types": ["node"]
  }
}